public class KeyWord {
	private String name;
	private String type;
	private int hitDocNum;
	private String date;
	private boolean isAvailable;
	private int state;//0为可用状态，1为分类内容少于8篇，2为长时间没有更新内容
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public int getHitDocNum() {
		return hitDocNum;
	}
	public void setHitDocNum(int hitDocNum) {
		this.hitDocNum = hitDocNum;
	}
	public String getDate() {
		return date;
	}
	public void setDate(String date) {
		this.date = date;
	}
	public boolean isAvailable() {
		return isAvailable;
	}
	public void setAvailable(boolean isAvailable) {
		this.isAvailable = isAvailable;
	}
	public int getState() {
		return state;
	}
	public void setState(int state) {
		this.state = state;
	}
}
