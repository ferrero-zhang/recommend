public class pcItemFront extends itemFront{

	@Expose
	@SerializedName("doctype")
	private String doctype;//item类型，如slide,doc,video
	
	@Expose
	@SerializedName("tags")
	private String tags;//编辑打的标签
	
	
	public String getDocType() {
		return doctype;
	}

	public void setDocType(String doctype) {
