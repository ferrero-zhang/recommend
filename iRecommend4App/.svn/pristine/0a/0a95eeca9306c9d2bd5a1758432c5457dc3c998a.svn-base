public class UserCenterModel {
	
	/*
	 * c"一级分类（足球，篮球）， "sc"二级分类（中国足球）， "cn"专题事件， "t"主题词， "s"稿源，  "s1" 少量优质栏目  "et"实体库词， 
	 * "kb"书名号中的词（游戏名，书名，电影名等）， "ks"冒号前的词（发言人，地区等） "kq"引号中的词（特指，特定词），"kr"分词得到的
	 * 人名，自定义实体词 "kl"分词得到的地名。
	 */
	
	/**
	 * @Fields user_id
	 */
	private String user_id;
	
	/**
	 * @Fields top_topic1 : c sc
	 * 看的最多|最近看的
	 * example: 足球#篮球|中国足球#欧洲足球
	 */
	private String top_topic1;
	
	/**
	 * @Fields top_topic2 : cn t s s1
	 */
	private String top_topic2;
	
	/**
	 * @Fields top_topic3 : et kb ks kq kr
	 */
	private String top_topic3;
		
	/**
	 * @Fields sort_interest_words : 
	 *  按时间降序的用户感兴趣词
	 */
	private String sort_interest_words;
	
	/**
	 * @Fields sort_book_words : 
	 *  按时间降序的用户订阅词
	 */
	private String sort_book_words;
	
	private String sort_keywords;
	
	private String sort_search_words;
	
	private String e_top_words;
	
	private String s1_top_words;


	/**
	 * @Fields user_active : 用户活跃度
	 */
	private String user_active;
	
	/**
	 * @Fields user_loc : 用户地理位置
	 */
	private String user_loc;
	/**
	 * @Fields user_os :
	 * 用户系统号
	 */
	private String user_os;
	

	/**
	 * @Fields user_ver :
	 * 用户客户端版本
	 */
	private String user_ver;
	private String user_mtype;
	private String sw_open;
	
	public String getSw_open() {
		return sw_open;
	}

	public void setSw_open(String sw_open) {
		this.sw_open = sw_open;
	}
	
	private String last_topic1;
	private String last_topic2;
	private String last_topic3;
	
	private String icar;
	private String igame;
	private String idigi;

	public UserCenterModel(String user_id, String top_topic1, String top_topic2, String top_topic3,
			String sort_interest_words, String sort_book_words, String user_active, String user_loc) {
		this.setUser_id(user_id);
		this.setTop_topic1(top_topic1);
		this.setTop_topic2(top_topic2);
		this.setTop_topic3(top_topic3);
		this.setSort_interest_words(sort_interest_words);
		this.setSort_book_words(sort_book_words);
		this.setUser_active(user_active);
		this.setUser_loc(user_loc);
	}

	public String getUser_id() {
		return user_id;
	}

	public void setUser_id(String user_id) {
		this.user_id = user_id;
	}

	public String getTop_topic1() {
		return top_topic1;
	}

	public void setTop_topic1(String top_topic1) {
		this.top_topic1 = top_topic1;
	}

	public String getTop_topic2() {
		return top_topic2;
	}

	public void setTop_topic2(String top_topic2) {
		this.top_topic2 = top_topic2;
	}

	public String getTop_topic3() {
		return top_topic3;
	}

	public void setTop_topic3(String top_topic3) {
		this.top_topic3 = top_topic3;
	}

	public String getUser_active() {
		return user_active;
	}

	public void setUser_active(String user_active) {
		this.user_active = user_active;
	}

	public String getUser_loc() {
		return user_loc;
	}

	public void setUser_loc(String user_loc) {
		this.user_loc = user_loc;
	}

	public String getSort_interest_words() {
		return sort_interest_words;
	}

	public void setSort_interest_words(String sort_interest_words) {
		this.sort_interest_words = sort_interest_words;
	}

	public String getSort_book_words() {
		return sort_book_words;
	}

	public void setSort_book_words(String sort_book_words) {
		this.sort_book_words = sort_book_words;
	}
	
	public String getUser_os() {
		return user_os;
	}

	public void setUser_os(String user_os) {
		this.user_os = user_os;
	}

	public String getUser_ver() {
		return user_ver;
	}

	public void setUser_ver(String user_ver) {
		this.user_ver = user_ver;
	}

	public String getUser_mtype() {
		return user_mtype;
	}

	public void setUser_mtype(String user_mtype) {
		this.user_mtype = user_mtype;
	}
	
	public String getSort_keywords() {
		return sort_keywords;
	}

	public void setSort_keywords(String sort_keywords) {
		this.sort_keywords = sort_keywords;
	}

	public String getSort_search_words() {
		return sort_search_words;
	}

	public void setSort_search_words(String sort_search_words) {
		this.sort_search_words = sort_search_words;
	}
	
	public String getE_top_words() {
		return e_top_words;
	}

	public void setE_top_words(String e_top_words) {
		this.e_top_words = e_top_words;
	}

	public String getS1_top_words() {
		return s1_top_words;
	}

	public void setS1_top_words(String s1_top_words) {
		this.s1_top_words = s1_top_words;
	}
	
	public String getLast_topic1() {
		return last_topic1;
	}

	public void setLast_topic1(String last_topic1) {
		this.last_topic1 = last_topic1;
	}

	public String getLast_topic2() {
		return last_topic2;
	}

	public void setLast_topic2(String last_topic2) {
		this.last_topic2 = last_topic2;
	}

	public String getLast_topic3() {
		return last_topic3;
	}

	public void setLast_topic3(String last_topic3) {
		this.last_topic3 = last_topic3;
	}

	public String getIcar() {
		return icar;
	}

	public void setIcar(String icar) {
		this.icar = icar;
	}

	public String getIgame() {
		return igame;
	}

	public void setIgame(String igame) {
		this.igame = igame;
	}

	public String getIdigi() {
		return idigi;
	}

	public void setIdigi(String idigi) {
		this.idigi = idigi;
	}
}
